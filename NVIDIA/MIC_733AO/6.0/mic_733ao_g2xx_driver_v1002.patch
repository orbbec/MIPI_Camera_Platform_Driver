+/*i2c orbbec,
+*orbbec_clientorbbeci2c_client
+*data
+*length
+*-10  
+*/
+static int i2c_write_orbbec(struct i2c_client *orbbec_client,void *data, u32 length)
+{
+	int error = 0;
+	struct i2c_msg send_msg; //
+
+	/* iic reg*/
+	send_msg.addr = orbbec_client->addr; //orbbec iic 
+	send_msg.flags = 0;					  //
+	send_msg.buf = (uint8_t*)data;			  //
+	send_msg.len = length;					  //reg
+
+	/**/
+	error = i2c_transfer(orbbec_client->adapter, &send_msg, 1);
+	if (error != 1)
+	{
+		printk(KERN_DEBUG "\n i2c_transfer error \n");
+		return -1;
+	}
+	return 0;
+}
+
+/*i2c orbbec
+*orbbec_clientorbbeci2c_client
+*data
+*length
+*-10
+*/
+static int i2c_read_orbbec(struct i2c_client *orbbec_client, void *data, u32 length)
+{
+	int error = 0;
+	struct i2c_msg orbbec_msg;
+
+	/*msg*/
+	orbbec_msg.addr = orbbec_client->addr;  //orbbec iic 
+	orbbec_msg.flags = I2C_M_RD;			//
+	orbbec_msg.buf = data;					//
+	orbbec_msg.len = length;				//
+
+	error = i2c_transfer(orbbec_client->adapter, &orbbec_msg, 1);
+
+	if (error != 1)
+	{
+		printk(KERN_DEBUG "\n i2c_read_orbbec error \n");
+		return -1;
+	}
+	return 0;
+}
