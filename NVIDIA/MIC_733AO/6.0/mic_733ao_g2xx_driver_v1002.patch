+static int orb_mux_get_fmt(struct v4l2_subdev *sd, 
+	#if LINUX_VERSION_CODE < KERNEL_VERSION(5, 15, 10)
+		struct v4l2_subdev_pad_config *cfg,
+	#else
+		struct v4l2_subdev_state *v4l2_state,
